      "type": "string"
    },
    "Author": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "excerpt": {
      "type": "text"
    },
    "articlecontent": {
      "type": "blocks"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "maincontent": {
      "type": "richtext"
    }
  }
}
